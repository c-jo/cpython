| !Boot file for RISC OS Python 3.

set Python$Dir <Obey$Dir>
IconSprites <Python$Dir>.!Sprites

Set Python3$Dir <Python$Dir>
Set Python3.8Lib$Dir <Python$Dir>.Lib
Set Python3.8DynLoad$Dir <Python$Dir>.Lib.lib-dynLoad

set Alias$@RunType_ae5 TaskWindow |"python3 %%*0|" -name |"Python|" -quit -wimpslot 16M
set File$Type_ae5 Python
set File$Type_aec PytnCode
set File$Type_aee PytnExtn

set Alias$python3 Run <Python$Dir>.python3 %*0